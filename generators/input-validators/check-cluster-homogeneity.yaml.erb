# Ignored keys for check-cluster-homogeneity.rb
# ---
# SYNTAX:
# sophia:
#   suno-[1-45]:
#     - ~main_memory.ram_size
#   sol-[1-50]:
#     - ~main_memory.ram_size
#   sol-29:
#     - +network_adapters.myri0
#
# nancy:
#   graphique: model differs on graphique-1
#   grisou-50+: 2 IB interfaces
#   grisou-49+: Doesn't have eth4 link
#   graphene-[45,85]: wrong BIOS version
#   graphene-104+: different wattmeter
#   griffon-[1-91]: missing some serial
#   griffon-[11-14]: additional myrinet interface
#
# grenoble:
#   edel: some nodes have only 1 IB interface (probably after a hardware replacement)
---
nancy:
  graphique-[1-2]:
    - ~gpu.gpu_model
    - ~gpu.gpu_cores
  grisou-49:
    - -kavlan.eth4
    - ~network_adapters.eth4.enabled
    - ~network_adapters.eth4.mountable
    - -network_adapters.eth4.rate
  grisou-50:
    - -kavlan.eth4
    - ~network_adapters.eth4.enabled
    - ~network_adapters.eth4.mountable
    - +network_adapters.ib0
  graphene-[45-46]:
    - ~bios.release_date
    - ~bios.version
  graphene-[86-87]:
    - ~bios.release_date
    - ~bios.version
  graphene-[104-144]:
    - ~monitoring.wattmeter
  griffon-[1-91]:
    - +chassis.serial
    - -chassis.serial
  griffon-11: # griffon-11 has an additional 10G NIC
    - +network_adapters.eth1
    - +network_adapters.eth1.rate
  griffon-14:
    - -network_adapters.eth1
    - -network_adapters.eth1.rate
  griffon-15:
    - -network_adapters.eth1
    - -network_adapters.eth1.rate
nantes:
  econome-[1-22]:
    - +network_adapters.eth2.rate
    - -network_adapters.eth2.rate
  econome-19:
    - ~network_adapters.eth0.firmware_version
    - ~network_adapters.eth1.firmware_version
rennes:
  parasilo-[1-28]:
    - ~storage_devices.sdf.model
    - ~storage_devices.sdf.rev
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  paranoia-[1-8]:
    - ~storage_devices.sd[b-e].model
    - ~storage_devices.sd[b-e].rev
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  parapide-[1-25]:
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  parapluie-[1-40]:
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
  paravance-[1-72]:
    - ~storage_devices.sd[a-e].by_id
    - ~storage_devices.sd[a-e].by_path
    - ~network_adapters.eth0.firmware_version
    - ~network_adapters.eth1.firmware_version
    - ~network_adapters.eth2.firmware_version
    - ~network_adapters.eth3.firmware_version
sophia:
  sol-29:
    - +network_adapters.myri0
  suno-2:
    - +chassis.serial
  uva-13:
    - -chassis.serial
  uvb-3:
    - -chassis.serial
    - ~chassis.manufacturer
  uvb-4:
    - +chassis.serial
    - ~chassis.manufacturer
  uvb-13:
    - -chassis.serial
    - ~chassis.manufacturer
  uvb-14:
    - +chassis.serial
    - ~chassis.manufacturer
grenoble:
  edel-[1-72]:
    - +network_adapters.ib1.guid
    - -network_adapters.ib1.guid
    - +network_adapters.ib1.model
    - -network_adapters.ib1.model
    - +network_adapters.ib1.vendor
    - -network_adapters.ib1.vendor
    - ~network_adapters.eth0.firmware_version
    - ~network_adapters.eth1.firmware_version
